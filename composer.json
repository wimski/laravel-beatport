{
    "name": "wimski/laravel-beatport",
    "description": "Beatport API for Laravel based on website requests",
    "keywords": ["laravel", "beatport", "api"],
    "homepage": "https://github.com/wimski/laravel-beatport",
    "authors": [
        {
            "name": "Wimski",
            "email": "thatonedudewimski@gmail.com"
        }
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "khill/php-duration": "^1.0",
        "myclabs/php-enum": "^1.7",
        "symfony/dom-crawler": "^5.1",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "cocur/slugify": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3",
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.5",
        "php-coveralls/php-coveralls": "^2.2",
        "swisnl/guzzle-fixture-handler": "^2.2",
        "haydenpierce/class-finder": "^0.4.2"
    },
    "autoload": {
        "psr-4": {
            "Wimski\\Beatport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wimski\\Beatport\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wimski\\Beatport\\Providers\\BeatportServiceProvider"
            ]
        }
    }
}
